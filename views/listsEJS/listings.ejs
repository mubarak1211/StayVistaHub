<div class="row mt-3">
    <% for(let each of data){ %>
      <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-4 listings-main mt-3" >
         <div class="card col" >
            <a href="/listings/view/<%=each._id%>">
            <img src="<%=each.image.url%>" class="card-img-top" alt="listing-image">
            <div class="card-img-overlay"></div>
               <div class="card-body">
               <h6 class="card-title"><%=each.title%></h6>
               <p class="card-text price">&#x20B9;<%=each.price.toLocaleString("en-IN")%>/night </p>
            </div>
            </a>
         </div>
      </div>
